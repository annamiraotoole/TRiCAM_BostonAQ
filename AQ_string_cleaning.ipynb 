   "source": [
    "def update_proportions(df):\n",
    "    for i in range(len(df[\"LU\"])):\n",
    "        line = json.dumps(df[\"LU\"][i])\n",
    "        my_dict = json.loads(line)\n",
    "        \n",
    "        \n",
    "        if 'forest' in my_dict:\n",
    "            df_state.loc[i, 'forest'] = df_state.loc[i, 'forest'] + my_dict['forest']\n",
    "\n",
    "        if 'open_land' in my_dict:\n",
    "            df_state.loc[i, 'open_land'] = df_state.loc[i, 'open_land'] + my_dict['open_land']\n",
    "\n",
    "\n",
    "        if 'water' in my_dict:\n",
    "            df_state.loc[i, 'water'] = df_state.loc[i, 'water'] + my_dict['water']\n",
    "\n",
    "\n",
    "        if 'wetland' in my_dict:\n",
    "            df_state.loc[i, 'wetland'] = df_state.loc[i, 'wetland'] + my_dict['wetland']\n",
    "\n",
    "\n",
    "        if 'saltwater/sandybeach' in my_dict:\n",
    "            df_state.loc[i, 'saltwater/sandybeach'] = df_state.loc[i, 'saltwater/sandybeach'] + my_dict['saltwater/sandybeach']\n",
    "\n",
    "\n",
    "        if 'recreational' in my_dict:\n",
    "            df_state.loc[i, 'recreational'] = df_state.loc[i, 'recreational'] + my_dict['recreational']\n",
    "\n",
    "\n",
    "        if 'marina' in my_dict:\n",
    "            df_state.loc[i, 'marina'] = df_state.loc[i, 'marina'] + my_dict['marina']\n",
    "\n",
    "\n",
    "        if 'transitional' in my_dict:\n",
    "            df_state.loc[i, 'transitional'] = df_state.loc[i, 'transitional'] + my_dict['transitional']\n",
    "\n",
    "\n",
    "        if 'urban_public/institution' in my_dict:\n",
    "            df_state.loc[i, 'urban_public/institution'] = df_state.loc[i, 'urban_public/institution'] + my_dict['urban_public/institution']\n",
    "\n",
    "\n",
    "        if \"commercial\" in my_dict:\n",
    "#             df['commercial'][i] = df['commercial'][i] + my_dict['commercial']\n",
    "            df_state.loc[i, 'commercial'] = df_state.loc[i, 'commercial'] + my_dict['commercial']\n",
    "\n",
    "\n",
    "        if \"transportation\" in my_dict:\n",
    "            df_state.loc[i, 'transportation'] = df_state.loc[i, 'transportation'] + my_dict['transportation']\n",
    "\n",
    "\n",
    "        if 'mining' in my_dict:\n",
    "            df_state.loc[i, 'mining'] = df_state.loc[i, 'mining'] + my_dict['mining']\n",
    "\n",
    "\n",
    "        if 'waste' in my_dict:\n",
    "            df_state.loc[i, 'waste'] = df_state.loc[i, 'waste'] + my_dict['waste']\n",
    "\n",
    "\n",
    "        if 'crop_land' in my_dict:\n",
    "            df_state.loc[i, 'crop_land'] = df_state.loc[i, 'crop_land'] + my_dict['crop_land']\n",
    "\n",
    "\n",
    "        if 'cemetary' in my_dict:\n",
    "            df_state.loc[i, 'cemetary'] = df_state.loc[i, 'cemetary'] + my_dict['cemetary']\n",
    "\n",
    "\n",
    "        if 'golfcourse' in my_dict:\n",
    "            df_state.loc[i, 'golfcourse'] = df_state.loc[i, 'golfcourse'] + my_dict['golfcourse']\n",
    "\n",
    "\n",
    "        if 'high_density_residential' in my_dict:\n",
    "            df_state.loc[i, 'high_density_residential'] = df_state.loc[i, 'high_density_residential'] + my_dict['high_density_residential']\n",
    "       \n",
    "        if 'medium_density_residential' in my_dict:\n",
    "            df_state.loc[i, 'medium_density_residential'] = df_state.loc[i, 'medium_density_residential'] + my_dict['medium_density_residential']\n",
    "\n",
    "        if 'low_density_residential' in my_dict:\n",
    "            df_state.loc[i, 'low_density_residential'] = df_state.loc[i, 'low_density_residential'] + my_dict['low_density_residential']\n",
    "\n",
    "        if 'industrial' in my_dict:\n",
    "            df_state.loc[i, 'industrial'] = df_state.loc[i, 'industrial'] + my_dict['industrial']\n",
    "\n",
    "    return df"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 332,
   "metadata": {},
   "outputs": [],
   "source": [
    "\"\"\"PM\"\"\"\n",
    "df_state = pd.read_csv(\"pm_final.csv\")\n",
    "df_w_cols = add_columns(df_state) \n",
    "col_w_names = change_names(df_w_cols[\"LU\"])\n",
    "df_w_props = update_proportions(df_w_cols)\n",
    "del df_w_props['LU']"
   ]
  },
