   "source": [
    "def add_columns(df):\n",
    "    df['forest'] = 0\n",
    "    df['open_land'] = 0\n",
    "    df['water'] = 0\n",
    "    df['wetland'] = 0\n",
    "    df['saltwater/sandybeach'] = 0\n",
    "    df['recreational'] = 0\n",
    "    df['marina'] = 0\n",
    "    df['transitional'] = 0\n",
    "    df['urban_public/institution'] = 0\n",
    "    df['commercial'] = 0\n",
    "    df['transportation'] = 0\n",
    "    df['mining'] = 0\n",
    "    df['waste'] = 0\n",
    "    df['crop_land'] = 0\n",
    "    df['cemetary'] = 0\n",
    "    df['golfcourse'] = 0\n",
    "    df['high_density_residential'] = 0\n",
    "    df['medium_density_residential'] = 0\n",
    "    df['low_density_residential'] = 0\n",
    "    df['industrial'] = 0\n",
    "    return df"
   ]
  },
